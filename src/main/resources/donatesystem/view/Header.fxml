      <Button layoutX="386.0" layoutY="33.0" mnemonicParsing="false" onAction="#directToAddRecordLanding" prefHeight="33.0" prefWidth="120.0" styleClass="button-outline" stylesheets="@style.css" text="Add New Record" textAlignment="CENTER" textFill="#355e59">
